		"strip-ansi": "^7.1.2",
		"wrap-ansi": "^9.0.2"
	},
	"devDependencies": {
		"@types/node": "^24.4.0",
		"string-width": "^8.1.0",
		"terminal.js": "^1.0.11",
		"tsd": "^0.33.0",
		"wcwidth": "^1.0.1",
		"xo": "^1.2.2"
	},
	"xo": {
		"rules": {
			"@typescript-eslint/no-unsafe-argument": "off",
			"import-x/no-extraneous-dependencies": "off",
